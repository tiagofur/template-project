# ==============================================
# DEVELOPMENT ENVIRONMENT
# ==============================================
# Local development configuration
# Copy to .env for local development

# ==============================================
# üåç ENVIRONMENT
# ==============================================
NODE_ENV=development
APP_ENV=development
DEBUG=true
LOG_LEVEL=debug

# ==============================================
# üåê APPLICATION URLS
# ==============================================
APP_NAME=My App Dev
APP_URL=http://localhost:3000
API_BASE_URL=http://localhost:3000/api
FRONTEND_URL=http://localhost:5173
BACKEND_URL=http://localhost:3000

# Ports
PORT=3000
FRONTEND_PORT=5173

# ==============================================
# üóÑÔ∏è DATABASE CONFIGURATION
# ==============================================
# PostgreSQL
DB_HOST=localhost
DB_PORT=5432
DB_NAME=myapp_dev
DB_USER=postgres
DB_PASSWORD=postgres
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/myapp_dev

# MongoDB (if using)
MONGODB_URI=mongodb://localhost:27017
MONGODB_DATABASE=myapp_dev
MONGODB_USERNAME=
MONGODB_PASSWORD=

# Redis Cache
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=

# ==============================================
# üîê AUTHENTICATION & SECURITY
# ==============================================
# JWT Configuration
JWT_SECRET=dev-jwt-secret-change-this-in-production-minimum-32-chars
JWT_ACCESS_SECRET=dev-access-secret-32-chars-minimum
JWT_REFRESH_SECRET=dev-refresh-secret-32-chars-minimum
JWT_EXPIRES_IN=15m
JWT_REFRESH_EXPIRES_IN=7d

# Session
SESSION_SECRET=dev-session-secret-change-in-production
SESSION_MAX_AGE=86400000

# Encryption
ENCRYPTION_KEY=dev-encryption-key-must-be-32-bytes-hexadecimal

# ==============================================
# üåê CORS & SECURITY
# ==============================================
CORS_ORIGIN=http://localhost:5173,http://localhost:3000
ALLOWED_ORIGINS=http://localhost:5173,http://localhost:3000,http://localhost:8080

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=1000

# Security Headers
HELMET_ENABLED=true
HSTS_MAX_AGE=31536000

# ==============================================
# ü§ñ AI SERVICES (Development Keys)
# ==============================================
# OpenAI
OPENAI_API_KEY=sk-dev-your-openai-api-key
OPENAI_MODEL=gpt-4
OPENAI_MAX_TOKENS=2000

# Anthropic Claude
ANTHROPIC_API_KEY=your-dev-anthropic-key

# Google AI
GOOGLE_AI_API_KEY=your-dev-google-ai-key

# ==============================================
# üîß EXTERNAL SERVICES (Test/Dev Keys)
# ==============================================
# GitHub Integration
GITHUB_TOKEN=ghp_your_dev_github_token
GITHUB_USERNAME=your_username
GITHUB_REPO_DEFAULT=your_repo

# OAuth Providers
GOOGLE_CLIENT_ID=your-dev-google-client-id
GOOGLE_CLIENT_SECRET=your-dev-google-client-secret
GOOGLE_CALLBACK_URL=http://localhost:3000/auth/google/callback

GITHUB_CLIENT_ID=your-dev-github-client-id
GITHUB_CLIENT_SECRET=your-dev-github-client-secret
GITHUB_CALLBACK_URL=http://localhost:3000/auth/github/callback

# ==============================================
# üìß EMAIL CONFIGURATION (Development)
# ==============================================
SMTP_HOST=smtp.mailtrap.io
SMTP_PORT=2525
SMTP_USER=your-mailtrap-user
SMTP_PASSWORD=your-mailtrap-password
SMTP_FROM=dev@localhost.com
EMAIL_ENABLED=false

# ==============================================
# ‚òÅÔ∏è CLOUD STORAGE (Development/Local)
# ==============================================
# AWS S3 (use test bucket)
AWS_ACCESS_KEY_ID=your-dev-access-key
AWS_SECRET_ACCESS_KEY=your-dev-secret-key
AWS_REGION=us-east-1
AWS_S3_BUCKET=myapp-dev-bucket

# Supabase
SUPABASE_URL=https://your-dev-project.supabase.co
SUPABASE_ANON_KEY=your-dev-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-dev-service-key

# ==============================================
# üí≥ PAYMENT PROVIDERS (Test Mode)
# ==============================================
# Stripe (Test Keys)
STRIPE_PUBLIC_KEY=pk_test_your_test_key
STRIPE_SECRET_KEY=sk_test_your_test_key
STRIPE_WEBHOOK_SECRET=whsec_test_your_webhook_secret

# ==============================================
# üìä MONITORING & ANALYTICS (Development)
# ==============================================
# Sentry (optional in dev)
SENTRY_DSN=
SENTRY_ENVIRONMENT=development
SENTRY_ENABLED=false

# Analytics (disabled in dev)
ANALYTICS_ID=
ANALYTICS_ENABLED=false

# Application Performance Monitoring
APM_ENABLED=false
NEW_RELIC_LICENSE_KEY=

# ==============================================
# üé≠ FEATURE FLAGS
# ==============================================
FEATURE_NEW_UI=true
FEATURE_BETA_FEATURES=true
FEATURE_EXPERIMENTAL=true
FEATURE_ANALYTICS=false
FEATURE_EMAIL_NOTIFICATIONS=false
FEATURE_WEBSOCKETS=true
FEATURE_PAYMENT_PROCESSING=false

# ==============================================
# üß™ TESTING & DEBUGGING
# ==============================================
# Testing
TEST_TIMEOUT=10000
TEST_DATABASE_URL=postgresql://postgres:postgres@localhost:5432/myapp_test
TEST_MONGODB_URI=mongodb://localhost:27017/myapp_test

# Browser Testing
PLAYWRIGHT_HEADLESS=false
PLAYWRIGHT_BROWSER=chromium
PLAYWRIGHT_TIMEOUT=30000
TEST_BROWSER_HEADLESS=false
TEST_VIEWPORT_WIDTH=1920
TEST_VIEWPORT_HEIGHT=1080

# Debugging
DEBUG_SQL=true
DEBUG_API=true
VERBOSE_LOGGING=true

# ==============================================
# üì± MOBILE DEVELOPMENT
# ==============================================
REACT_NATIVE_ENV=development
FLUTTER_ENV=development
FLUTTER_WEB_PORT=5000
FLUTTER_WEB_HOST=localhost

# ==============================================
# üê≥ DOCKER & DEVELOPMENT TOOLS
# ==============================================
# PgAdmin
PGADMIN_EMAIL=admin@admin.com
PGADMIN_PASSWORD=admin
PGADMIN_PORT=5050

# Redis Commander
REDIS_COMMANDER_PORT=8081

# ==============================================
# üîÑ CACHE CONFIGURATION
# ==============================================
CACHE_ENABLED=true
CACHE_TTL=3600
CACHE_PREFIX=dev:

# ==============================================
# üìù LOGGING
# ==============================================
LOG_FILE=logs/dev.log
LOG_FORMAT=dev
LOG_SQL_QUERIES=true
LOG_HTTP_REQUESTS=true

# ==============================================
# üöÄ BUILD & DEPLOYMENT
# ==============================================
BUILD_PATH=dist
SOURCE_MAP=true
MINIFY=false

# ==============================================
# üåç LOCALIZATION
# ==============================================
DEFAULT_LANGUAGE=en
SUPPORTED_LANGUAGES=en,es,pt
TIMEZONE=UTC

# ==============================================
# üìä PERFORMANCE
# ==============================================
MAX_REQUEST_SIZE=10mb
REQUEST_TIMEOUT=30000
CONNECTION_POOL_SIZE=10
